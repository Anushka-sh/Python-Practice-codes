#include<stdio.h>
void main()
{
 int i, j, k, x, y, z, c[100], n;
 
 printf("Enter the size of the array:\n");
 scanf("%d", &n);
 
 printf("Enter the elements in array:\n");
 for(i=0;i<n;i++)
 scanf("%d", &c[i])
 
 for(i=0;i<n;i++)
  for(j=i+1;j<n;j++)
    for(k=j+1;k<n;k++)
      x = c[i]*c[i]
      y = c[j]*c[j]
      z = c[k]*c[k]
if(x==y+z||y==x+z||z==x+y)
 printf("True\n");
else
 printf("false\n");
}
